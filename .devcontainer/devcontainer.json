{
  "name": "Python 3.11 with Jupyter",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-toolsai.jupyter"
      ]
    }
  },
  "features": {
    "ghcr.io/devcontainers/features/common-utils:2": {
      "installZsh": "true",
      "username": "vscode"
    }
  },
  "remoteUser": "vscode",
  "runArgs": [
    "--env",
    "WSL_MNT_WAYLAND=/tmp/not-a-wayland-socket"
  ]
}